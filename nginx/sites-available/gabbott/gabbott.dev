server {
        server_name gabbott.dev www.gabbott.dev ;
        root /var/www/gabbott/gabbott;
        index index.html index.htm index.nginx-debian.html ;

        location / {
                try_files $uri $uri/ =404 ;
        }

        listen 80 ; 
        listen [::]:80;
        server_name gabbott.dev www.gabbott.dev

}
